coding:
l=[]
n = int(input())
for i in range(n):
    s = input()
    for i in s:
        if i.isalpha():
            l.append(s.index(i))
for i in range(len(l)): 
    try:
        if l[i] == l[i+1]:
            print("Down")
        elif l[i]<l[i+1]:
            print("Right")
        elif l[i]>l[i+1]:
            
            print("Left")
    except:
        pass
