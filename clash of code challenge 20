import sys
import math

# Auto-generated code below aims at helping you parse
# the standard input according to the problem statement.
x=""
m = int(input())
if m<0 or m==0:
    print(0)

else:
    for i in range(m+1):
        if i%2!=0:
           x=x+str(i)+" "
    
    print(x.strip())





