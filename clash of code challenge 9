import sys
import math

# Auto-generated code below aims at helping you parse
# the standard input according to the problem statement.

n = int(input())
for i in range(n):
    e = input()
    if e=="FRIEND EARTH":
        print("HEAL EARTH")
    elif e=="FRIEND WATER":
        print("HEAL WATER")
    elif e=="FRIEND FIRE":
        print("HEAL FIRE")
    elif e=="FOE EARTH":
        print("ATTACK FIRE")
    elif e=="FOE WATER":
        print("ATTACK EARTH")
    elif e=="FOE FIRE":
        print("ATTACK WATER")


